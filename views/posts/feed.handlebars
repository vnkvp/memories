<div class="feed">
  {{#each posts}}
  <div class="card mb-3">
    <div class="profiletag">
      <p>{{username}}</p>
    </div>
    <a href="/uploads/{{image}}" data-lightbox="gallery"><img src="/uploads/{{image}}" class="img-fluid"></a>
    <div class="card-body">
    <h4>{{title}}</h4>
    <p>{{details}}</p>
    </div>
  </div>
  {{else}}
  <p>No pictures listed</p>
  {{/each}}
</div>
